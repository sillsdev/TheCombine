apiVersion: v1
kind: ConfigMap
metadata:
  creationTimestamp: null
  name: {{ proxy_server_attr.env_name }}
  namespace: {{ cert_proxy_namespace }}
data:
  AWS_S3_BUCKET: s3://{{ aws_s3_loc }}/certs
  NUC_CERTIFICATES: {{ combine_cert_proxy_list | default([]) | join(" ") }}
  SERVER_NAME: {{ cert_proxy_hostname }}
