apiVersion: v1
data:
  CERT_ADDL_DOMAINS: '[]'
  CERT_EMAIL: "{{ cert_email }}"
  CERT_MODE: "{{ cert_mode }}"
  CERT_PROXY_DOMAINS: '[]'
  CERT_PROXY_RENEWAL: "60"
  CERT_SELF_RENEWAL: "{{ cert_self_renewal | default(30) }}"
  CERT_STAGING: "0"
  MAX_CONNECT_TRIES: "15"
  SERVER_NAME: {{ combine_server_name }}
kind: ConfigMap
metadata:
  creationTimestamp: null
  labels:
    io.kompose.service: certmgr-env-certmgr
  name: env-certmgr
