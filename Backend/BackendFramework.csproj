<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AnalysisMode>Recommended</AnalysisMode>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);CA1305;CA1816;CA1848;CS1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Data\ddp4-ar.json" />
    <None Remove="Data\ddp4-en.json" />
    <None Remove="Data\ddp4-es.json" />
    <None Remove="Data\ddp4-fr.json" />
    <None Remove="Data\ddp4-hi.json" />
    <None Remove="Data\ddp4-ml.json" />
    <None Remove="Data\ddp4-my.json" />
    <None Remove="Data\ddp4-pt.json" />
    <None Remove="Data\ddp4-ru.json" />
    <None Remove="Data\ddp4-sw.json" />
    <None Remove="Data\ddp4-zh.json" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Data\ddp4-ar.json" />
    <EmbeddedResource Include="Data\ddp4-en.json" />
    <EmbeddedResource Include="Data\ddp4-es.json" />
    <EmbeddedResource Include="Data\ddp4-fr.json" />
    <EmbeddedResource Include="Data\ddp4-hi.json" />
    <EmbeddedResource Include="Data\ddp4-ml.json" />
    <EmbeddedResource Include="Data\ddp4-my.json" />
    <EmbeddedResource Include="Data\ddp4-pt.json" />
    <EmbeddedResource Include="Data\ddp4-ru.json" />
    <EmbeddedResource Include="Data\ddp4-sw.json" />
    <EmbeddedResource Include="Data\ddp4-zh.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="RelaxNG" Version="3.2.3" >
      <NoWarn>NU1701</NoWarn>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.24" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.33.0" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.33.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
    <PackageReference Include="MongoDB.Driver" Version="2.22.0" />
    <PackageReference Include="MailKit" Version="4.2.0" />
    <PackageReference Include="Xabe.FFmpeg" Version="5.2.6"/>

    <!-- SIL Maintained Dependencies. -->
    <PackageReference Include="icu.net" Version="2.9.0" />
    <PackageReference Include="Icu4c.Win.Full.Lib" Version="62.2.1-beta" />
    <PackageReference Include="SIL.Core" Version="12.0.1" />
    <PackageReference Include="SIL.Core.Desktop" Version="12.0.1">
      <NoWarn>NU1701</NoWarn>
    </PackageReference>
    <PackageReference Include="SIL.DictionaryServices" Version="12.0.1">
      <NoWarn>NU1701</NoWarn>
    </PackageReference>
    <PackageReference Include="SIL.Lift" Version="12.0.1">
      <NoWarn>NU1701</NoWarn>
    </PackageReference>
    <PackageReference Include="SIL.WritingSystems" Version="12.0.1" />
  </ItemGroup>
</Project>
